{
  "user": "admin",
  "txTimeout": 300000,
  "skipInit": true,
  "channel": "ch1",
  "keyStore": "<%= root %>/.hfc-<%= org %>",
  "//**INSIDE-DOCKER**//keyStore": "/config/.hfc-<%= org %>",
  "networkProfile": "<%= root %>/network-profiles/<%= org %>.network-profile.yaml",
  "//**INSIDE-DOCKER**//networkProfile": "/config/network-profiles/<%= org %>.network-profile.inside-docker.yaml",
  "controllers": [
    {
      "name": "<%- chaincodeFolder %>",
      "version": "file:./packages/<%- chaincodeFolder %>",
      "controller": "<%- className %>Controller"
    }
  ],
  "policy": {
    "identities": [
      { "role": { "name": "member", "mspId": "Org1MSP" }},
      { "role": { "name": "member", "mspId": "Org2MSP" }}
    ],
    "policy": {
      "1-of": [{ "signed-by": 0 }, { "signed-by": 1 }]
    }
  }
}